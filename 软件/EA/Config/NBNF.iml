<?xml version="1.0"?>
<intellisenseDB>
    <intellisenseTriggers calltipStart="(" calltipEnd=")" accessorToken="&lt;"/>
    <intellisenseItems>
        <!-- RULE -->
        <!-- These will be all automatically added.. <intellisenseItem -->
